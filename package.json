{
  "name": "orbit-ai",
  "displayName": "OrbitAI",
  "description": "A WebSocket client for VSCode to connect and interact with WebSocket servers in real-time.",
  "version": "1.0.0",
  "publisher": "khanhromvn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/khanhromvn/orbit-ai.git"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "websocket",
    "client",
    "realtime"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "websocket-container",
          "title": "WebSocket",
          "icon": "$(plug)"
        }
      ]
    },
    "views": {
      "websocket-container": [
        {
          "type": "webview",
          "id": "websocketView",
          "name": "WebSocket Client"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.50.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "ws": "^8.18.3"
  }
}
